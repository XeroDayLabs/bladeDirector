<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="servicesSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="servicesSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost/bladedirector/services.asmx"
                binding="basicHttpBinding" bindingConfiguration="servicesSoap"
                contract="networkService.servicesSoap" name="servicesSoap" />
            <endpoint address="http://localhost/bladedirector/services.asmx"
                binding="customBinding" bindingConfiguration="servicesSoap12"
                contract="networkService.servicesSoap" name="servicesSoap12" />
        </client>
    </system.serviceModel>
</configuration>